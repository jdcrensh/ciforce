{
  "name": "sfdc-build",
  "version": "1.0.0",
  "description": "Build script for CI with SalesForce",
  "author": "jon@redteal.com",
  "license": "ISC",
  "main": "build.coffee",
  "repository": {
    "type": "git",
    "url": "ssh://stash.ecoact.org:7999/eo/ant-build.git"
  },
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "async": "^1.4.2",
    "coffee-script": "^1.10.0",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.14",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "jsforce": "^1.5.0",
    "load-grunt-tasks": "^3.2.0",
    "lodash": "^3.10.1",
    "minimatch": "^2.0.10",
    "pretty-data": "^0.40.0",
    "simple-git": "^1.8.0",
    "time-grunt": "^1.2.1",
    "xmldom": "^0.1.19",
    "xmlpoke": "^0.1.10",
    "xpath": "^0.0.9"
  },
  "sfdc": {
    "version": "33.0",
    "checkOnly": true,
    "allowMissingFiles": true,
    "ignoreWarnings": true,
    "maxPoll": 1000,
    "pollWaitMillis": 5000,
    "testReportsDir": "build/test-reports",
    "fullDeploy": false,
    "undeployMissing": true
  },
  "defaultExcludes": {
    "Layout": [
      "FeedItem-Feed Item Layout",
      "Question-Question Layout",
      "Reply-Reply Layout",
      "SocialPersona-Social Persona Layout"
    ],
    "CustomObject": [
      "SocialPersona"
    ],
    "Workflow": [
      "SocialPost",
      "ExternalEventMapping",
      "Question",
      "Reply",
      "SocialPersona"
    ],
    "Role": [
      "EnlightCustomerExecutive",
      "EnlightCustomerManager",
      "EnlightCustomerUser"
    ],
    "Settings": [
      "PathAssistant",
      "PersonalJourney"
    ]
  }
}
